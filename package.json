{
  "name": "react-bulma-companion",
  "version": "0.5.2",
  "description": "React Components for Bulma",
  "main": "lib/index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack --mode=development --watch",
    "prebuild": "rm -rf dist && rm -rf lib",
    "build": "npm run build:lib && npm run build:dev && npm run build:prd",
    "build:dev": "NODE_ENV=development webpack --mode=production",
    "build:prd": "NODE_ENV=production webpack --mode=production",
    "build:lib": "babel src --out-dir lib --ignore 'src/**/*.test.js'",
    "build:docs": "rm -rf docs && storybook build --docs -o docs",
    "deploy:docs": "surge docs react-bulma-companion.surge.sh",
    "storybook": "storybook dev -p 8008",
    "storybook:docs": "storybook dev -p 8008 --docs",
    "test": "jest",
    "lint": "eslint src"
  },
  "pre-commit": [
    "test",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djizco/react-bulma-companion.git"
  },
  "author": {
    "name": "Federico Rojas",
    "email": "djizco@yahoo.com",
    "url": "https://github.com/djizco"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "bulma",
    "components"
  ],
  "files": [
    "dist/react-bulma-companion.js",
    "dist/react-bulma-companion.min.js",
    "lib"
  ],
  "dependencies": {
    "classnames": "2.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.21.5",
    "@babel/core": "7.21.8",
    "@babel/eslint-parser": "7.21.8",
    "@babel/preset-env": "7.21.5",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.5",
    "@fortawesome/fontawesome-svg-core": "6.4.0",
    "@fortawesome/free-regular-svg-icons": "6.4.0",
    "@fortawesome/free-solid-svg-icons": "6.4.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@storybook/addon-essentials": "7.0.9",
    "@storybook/react": "7.0.9",
    "@storybook/react-webpack5": "7.0.9",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.0.0",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "babel-loader": "8.2.5",
    "bulma": "0.9.4",
    "eslint": "8.40.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.12",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "pre-commit": "1.2.2",
    "prop-types": "15.8.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "storybook": "7.0.9",
    "typescript": "5.0.4",
    "webpack": "5.82.0",
    "webpack-bundle-analyzer": "4.8.0",
    "webpack-cli": "5.1.0"
  },
  "peerDependencies": {
    "bulma": "0.9.x",
    "prop-types": ">=15.7.2",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  }
}
