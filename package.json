{
  "name": "react-bulma-companion",
  "version": "0.0.1",
  "description": "React Components for Bulma",
  "main": "dist/react-bulma-companion.min.js",
  "scripts": {
    "start": "webpack --watch",
    "build": "rm -rf dist && npm run build:lib && npm run build:development && npm run build:production",
    "build:development": "NODE_ENV=development webpack --mode=development",
    "build:production": "NODE_ENV=production webpack --mode=production",
    "build:lib": "rm -rf lib && babel src --out-dir lib --ignore 'src/**/*.test.js'",
    "test": "jest",
    "lint": "eslint src"
  },
  "pre-commit": ["test", "lint"],
  "author": {
    "name": "Federico Rojas",
    "email": "djizco@yahoo.com",
    "url": "https://github.com/djizco"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "bulma",
    "components"
  ],
  "files": [
    "dist/react-bulma-companion.js",
    "dist/react-bulma-companion.min.js",
    "lib"
  ],
  "dependencies": {
    "classnames": "2.2.6"
  },
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "eslint": "7.9.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-import-resolver-webpack": "0.12.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.21.1",
    "eslint-plugin-react-hooks": "4.1.2",
    "jest": "26.4.2",
    "jest-environment-enzyme": "7.1.2",
    "pre-commit": "1.2.2",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "webpack": "4.44.2",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-cli": "3.3.12"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "prop-types": ">= 15.7.2",
    "bulma": "0.9.0"
  }
}
